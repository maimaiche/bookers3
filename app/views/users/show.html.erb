<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
    <% if @user == current_user %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>
<!-- ユーザーの投稿一覧 -->
<% @books.each do |book| %>
<div>
  <p>Title : <%=book.title %></p>
  <p>introduction：<%= book.body %></p>
  <p>name：<%= book.user.name %></p>
</div>
<% end %>